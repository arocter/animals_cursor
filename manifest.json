{
	"manifest_version": 2,
	"name": "Animal Cursors",
	"version": "1.0",

	"browser_action":{
		"default_title": "Change Cursor"
	},

	"content_scripts":[{
		"matches": ["<all_urls>"],
		"js":["jquery-2.2.4.min.js","content.js"]
	}],

	"background":{
		"scripts": ["background.js"]
	},

	"permissions":[
		"tabs"
	],

	"web_accessible_resources":[
		"*.jpg"
	]
}